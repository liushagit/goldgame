<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ProxyServlet</servlet-name>
  	<display-name>ProxyServlet</display-name>
  	<description></description>
  	<servlet-class>com.orange.goldgame.servlet.ProxyServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>ConsumServlet</servlet-name>
  	<display-name>ConsumServlet</display-name>
  	<description></description>
  	<servlet-class>com.orange.goldgame.servlet.ConsumServlet</servlet-class>
  	<load-on-startup>9</load-on-startup>
  </servlet>
  
  <!-- =================================== -->
  <servlet-mapping>
  	<servlet-name>ProxyServlet</servlet-name>
  	<url-pattern>/p</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>ConsumServlet</servlet-name>
  	<url-pattern>/c</url-pattern>
  </servlet-mapping>
</web-app>
